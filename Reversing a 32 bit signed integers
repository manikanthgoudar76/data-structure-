#include <stdio.h>
int reverseInteger(int num) 
{
    int reversed = 0;
    while (num != 0) 
    {
        int digit = num % 10;
        reversed = reversed * 10 + digit;
        num /= 10;
    }
    return reversed;
}
int main() 
{
    int num = -2 4 6 8 10 12;
    int reversedNum = reverseInteger(num);
    printf("Reversed Integer: %d\n", reversedNum);
    return 0;
}

Output:
Reversed Integer:12 10 8 6 4 2  -
