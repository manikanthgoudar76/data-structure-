#include<stdio.h>
int fibonacci(int n){
    if(n<=1)
    return n;
    else
    return fibonacci(n-1)+fibonacci(n-2);
}
int main(){
    int n,i;
    printf("enter the num of terms:");
    scanf("%d",&n);
    printf("fibonacci series:");
    for(i = 0; i < n;i++){
        printf("%d",fibonacci(i));
    }
    return 0;
}
input:enter the num of terms:5
output:fibonacci series:01123
