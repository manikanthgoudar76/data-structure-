#include<stdio.h>
int main(){
    int fact=1,num,i;
    printf("enter any number:");
    scanf("%d",&num);
    for(i = 2; i <=num; i++){
        fact*=i;
    }
    printf("the factorial of the given number is:%d",fact);
    return 0;
}
input:enter any number:5
output:the factorial of the given number is:120
