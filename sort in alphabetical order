#include <stdio.h>
#include <string.h>
int main() 
{
    char str[] = "monkey";
    int n = strlen(str);
    
    for (int i = 0; i < n - 1; i++) 
{
        for (int j = i + 1; j < n; j++) 
{
            if (strcmp(&str[i], &str[j]) > 0)
{
                char temp = str[i];
                str[i] = str[j];
                str[j] = temp;
}
}
}
    printf("Sorted string: %s\n", str);    
    return 0;
}

Output:
Sorted string: ekmnoy
