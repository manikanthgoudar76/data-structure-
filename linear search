#include <stdio.h>
int search(int arr[],int n,int x){
    for(int i=0;i<x;i++){
        if(arr[i]==n){
            return i;
        }
    }
}
int main() {
    int arr[]={2,3,5,7,8,10,12,15,28,20};
    int n=7;
    int x=sizeof(arr)/sizeof(arr[0]);
    printf("%d is present in index %d",n,search(arr,n,x));
    return 0;
}

input:2,3,5,7,8,10,12,15,28,20
output:7 is present in index 3
