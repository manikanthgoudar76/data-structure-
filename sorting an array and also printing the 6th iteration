#include <stdio.h>
int main(){
    int arr[]={90,70,20,34,6,23,42,10,86,34,22};
    int n=sizeof(arr)/sizeof(arr[0]);
    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++){
            if(arr[i]>arr[j]){
                int temp=arr[i];
                arr[i]=arr[j];
                arr[j]=temp;
            }
        }
        if(i==6){
            printf("The array after 6th iteration:\n");
            for(int i=0;i<n;i++){
                printf("%d ",arr[i]);
            }
            printf("\n");
        }
    }
    printf("The sorted array\n");
    for(int i=0;i<n;i++){
        printf("%d ",arr[i]);
    }
    return 0;
}
input:90,70,20,34,6,23,42,10,86,34,22

output:
The array after 6th iteration:
6 10 20 22 23 34 34 90 86 70 42 
The sorted array
6 10 20 22 23 34 34 42 70 86 90 
